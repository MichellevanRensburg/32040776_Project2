@{
    ViewData["Title"] = "Manage Image";
}

<div class="row">
    <div class=" = " col-md-5">
        <form method="post" enctype="multipart/form-data" asp-controlller="Image" asp-action="Index">
            <div class="form-group">
                <div class="=col-md-10">
                    <p>Upload Image</p>
                    <input class="form-control" name="image" type="image" />
                </div>
            </div>
            <div class="form-group">
                <div class="=col-md-10">
                    <input class="btn btn-success" type="submit" value="Upload" />
                </div>
            </div>
        </form>

    </div>
</div>
